{
  "name": "mobx-restful-migrator",
  "version": "0.1.0",
  "license": "LGPL-3.0-or-later",
  "author": "shiy2008@gmail.com",
  "description": "Data Migration framework based on MobX-RESTful",
  "keywords": [
    "mobx",
    "restful",
    "data",
    "migration",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idea2app/MobX-RESTful-migrator.git"
  },
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "dependencies": {
    "mobx-restful": "^2.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.35.0",
    "jest": "^29.7.0",
    "jiti": "^2.5.1",
    "koajax": "^3.1.2",
    "mobx": "^6.13.7",
    "ts-jest": "^29.4.3",
    "typescript": "^5.9.2",
    "web-utility": "^4.6.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "printWidth": 100
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepare": "npm run build"
  }
}
