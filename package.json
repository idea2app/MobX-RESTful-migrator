{
  "name": "mobx-restful-migrator",
  "version": "0.1.0",
  "license": "LGPL-3.0-or-later",
  "author": "shiy2008@gmail.com",
  "description": "Data Migration framework based on MobX-RESTful",
  "keywords": [
    "mobx",
    "restful",
    "data",
    "migration",
    "crawler",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idea2app/MobX-RESTful-migrator.git"
  },
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "dependencies": {
    "mobx-restful": "^2.1.2"
  },
  "peerDependencies": {
    "core-js": ">=3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.6",
    "core-js": "^3.45.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "jiti": "^2.5.1",
    "koajax": "^3.1.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "web-utility": "^4.6.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "printWidth": 100
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "scripts": {
    "test": "eslint src --ext .ts --fix  &&  jest",
    "dev": "tsc --watch",
    "build": "rm -rf dist/  &&  tsc",
    "prepublishOnly": "npm test  &&  npm run build"
  }
}
